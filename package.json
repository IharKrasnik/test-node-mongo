{
  "name": "koa-api-starter",
  "version": "1.4.0",
  "description": "Koa api",
  "private": false,
  "main": "src/app.js",
  "author": "Paralect",
  "keywords": [
    "koa",
    "rest api"
  ],
  "scripts": {
    "build-emails": "gulp mail",
    "test": "npm run lint && NODE_ENV=test mocha --timeout 20s --recursive --exit --colors src/tests/app.spec.js",
    "development": "gulp mail && NODE_ENV=development nodemon --inspect=0.0.0.0:3101 --watch src src/app.js",
    "start": "node src/app.js",
    "schedule": "node src/scheduler.js",
    "schedule-dev": "nodemon --watch src src/scheduler.js",
    "migrate": "node src/migration.js",
    "lint": "eslint ./src"
  },
  "dependencies": {
    "@elastic/elasticsearch": "7.0.0-rc.1",
    "@google-cloud/vision": "0.21.0",
    "@paralect/common-logger": "0.1.10",
    "@paralect/node-mongo": "0.3.1",
    "app-module-path": "2.2.0",
    "async": "2.6.0",
    "async-busboy": "0.6.2",
    "aws-sdk": "2.172.0",
    "axios": "0.18.0",
    "base64url": "2.0.0",
    "bcrypt": "3.0.2",
    "chai": "4.2.0",
    "chargebee": "2.4.6",
    "co": "4.6.0",
    "cron": "1.4.1",
    "csv-parse": "2.0.4",
    "csvtojson": "2.0.8",
    "format-number-with-string": "0.0.2",
    "gulp": "3.9.1",
    "gulp-inline-css": "3.1.1",
    "handlebars": "4.0.11",
    "iconv-lite": "0.4.24",
    "image-size": "0.6.2",
    "intercom-client": "2.10.4",
    "joi": "13.0.1",
    "json2csv": "5.0.7",
    "jsonwebtoken": "8.1.0",
    "jszip": "3.1.5",
    "kcors": "2.2.1",
    "koa": "2.4.1",
    "koa-basic-auth": "3.0.0",
    "koa-bodyparser": "4.2.0",
    "koa-helmet": "3.2.0",
    "koa-logger": "3.1.0",
    "koa-mount": "3.0.0",
    "koa-router": "7.2.1",
    "lodash": "4.17.4",
    "mailgun-js": "0.16.0",
    "moment": "2.19.4",
    "moment-duration-format": "2.2.2",
    "moment-timezone": "0.5.21",
    "mongodb": "3.0.1",
    "node-polyglot": "2.2.2",
    "nodemon": "1.12.1",
    "numeral": "2.0.6",
    "qrcode": "1.2.0",
    "request": "2.83.0",
    "request-promise-native": "1.0.5",
    "sanitize-html": "1.17.0",
    "socket.io": "2.1.1",
    "socket.io-emitter": "3.1.1",
    "socket.io-redis": "5.2.0",
    "tunnel-ssh": "4.1.4",
    "validate-vat": "0.5.1",
    "xlsx": "0.12.1",
    "xml2js": "0.4.19"
  },
  "devDependencies": {
    "@paralect/eslint-config": "0.1.5",
    "babel-eslint": "8.0.1",
    "eslint": "4.10.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.4.0",
